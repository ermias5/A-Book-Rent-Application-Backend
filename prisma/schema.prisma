generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  BASIC
  OWNER
  ADMIN
}

model User {
  id    Int     @id @default(autoincrement())
  name  String? @unique
  email String
  password String 
  PhoneNumber String?
  location String?
  role Role @default(BASIC)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
