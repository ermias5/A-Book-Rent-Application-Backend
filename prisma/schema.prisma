generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  email     String
  name      String
  role      Role     @default(BASIC)
  createdAt DateTime @default(now())
  password  String
  updatedAt DateTime @updatedAt
}

enum Role {
  BASIC
  OWNER
  ADMIN
}
